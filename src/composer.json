{
    "name": "campus/unit_test",
    "description": "Package to learn how to create unit Test with PHPunit",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Campus\\UnitTest\\": "src/"
        }
    },
